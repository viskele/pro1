int lengthOfLongestSubstring(char* s) {
    int freq[256] = {0};
    int left = 0, right = 0, maxLen = 0;

    while (s[right] != '\0') {
        freq[(unsigned char)s[right]]++;
        
        while (freq[(unsigned char)s[right]] > 1) {
            freq[(unsigned char)s[left]]--;
            left++;
        }

        int currLen = right - left + 1;
        if (currLen > maxLen)
            maxLen = currLen;

        right++;
    }
    return maxLen;
}
